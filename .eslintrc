{
  "extends": [
    "react-app",
    "react-app/jest",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "block-scoped-var": "warn",
    "camelcase": "warn",
    "curly": [
      "error",
      "all"
    ],
    "react/prop-types": "warn",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": false
      }
    ]
  }
}